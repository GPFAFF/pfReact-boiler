{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars": 0,
    "max-len": [1, 120, 2, {
      "ignoreComments": true
    }],
    "prop-types": [1]
  },
  "settings": {
    "react": {
      "version": "^16.6.1"
    },
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx"]
      }
    }
  },
  "plugins": [
    "react"
  ],
  "extends": ["airbnb-base", "jest-enzyme"]
}